public class ApiCaller {
    private String baseUrl;
    private String apiKey;
    private String sharedSecret;
    private String ipAddress;

    public ApiCaller(String baseUrl, String apiKey, String sharedSecret, String ipAddress) {
        this.baseUrl = baseUrl;
        this.apiKey = apiKey;
        this.sharedSecret = sharedSecret;
        this.ipAddress = ipAddress;
    }

    public void callApi() {
        try {
            // Call the API
            GdaResponse response = GeoPointService.callGeoPointLookup(baseUrl, apiKey, sharedSecret, ipAddress);
            // Print the response to the console
            System.out.println("API Response: " + response.toString());
        } catch (DataFormatException | Aconsele e) {
            e.printStackTrace(); // Handle exceptions as needed
        }
    }
}
